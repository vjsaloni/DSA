class Solution{
  public:
    int Countpair(int arr[], int n, int sum){
        int count=0;
        for(int i=0;i<n;i++){
            for(int j=i+1;j<n;j++){
              if(sum==arr[i]+arr[j])
            
                count++;
            }
            
        }
        if(count==0)
           return -1;
        else
           return count;
    
        
    }
};
